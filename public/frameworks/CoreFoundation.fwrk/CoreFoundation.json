{
    "compiler_path": "x86_64-w64-mingw32-g++",
    "compiler_std": "c++20",
    "headers_path": ["../", "./", "../../../dev", "../../../dev/kernel"],
    "sources_path": ["src/*.cc"],
    "output_name": "./dist/libCoreFoundation.fwrk.dylib",
    "compiler_flags": [
      "-ffreestanding",
      "-shared",
      "-fno-rtti",
      "-fno-exceptions",
      "-Wl,--subsystem=17"
    ],
    "cpp_macros": [
      "kCFVersion=0x0100",
      "kCFVersionHighest=0x0100",
      "kCFVersionLowest=0x0100",
      "__NE_AMD64__",
      "__CF_64BIT__"
    ]
  }
  