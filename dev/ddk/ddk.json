{
  "compiler_path": "x86_64-w64-mingw32-gcc",
  "compiler_std": "c++20",
  "headers_path": ["../", "./"],
  "sources_path": ["*.c", "*.cxx", "*.S"],
  "output_name": "ddk.dll",
  "compiler_flags": [
    "-ffreestanding",
    "-shared",
    "-std=c17",
    "-std=c++20",
    "-fno-rtti",
    "-fno-exceptions",
    " -Wl,--subsystem=17"
  ],
  "cpp_macros": [
    "__NEWOSKRNL__",
    "__DDK_AMD64__",
    "__DDK__",
    "cDDKVersionHighest=0x0100",
    "cDDKVersionLowest=0x0100",
    "cDDKVersion=0x0100"
  ]
}
