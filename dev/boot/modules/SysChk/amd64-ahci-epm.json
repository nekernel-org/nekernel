{
  "compiler_path": "x86_64-w64-mingw32-g++",
  "compiler_std": "c++20",
  "headers_path": ["../", "../../", "../../../kernel", "../../../", "./"],
  "sources_path": [
    "*.cc",
    "*.S",
    "../../src/HEL/AMD64/BootSATA.cc",
    "../../src/HEL/AMD64/BootPlatform.cc",
